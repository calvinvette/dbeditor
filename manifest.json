{
  "name": "DBEditor",
  "version": "0.1.3",
  "manifest_version": 2,
  "description": "Enhance the editor functionality of Databricks notebooks",
  "icons": {
    "48": "logo.png"
  },
  "content_scripts": [
    {
      "matches": ["https://*.databricks.com/*"],
      "js": ["content-script.js"],
      "run_at": "document_idle"
    }
  ],
  "web_accessible_resources": ["main.js"]
}
